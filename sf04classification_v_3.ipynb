     "output_type": "execute_result"
    }
   ],
   "source": [
    "best_model.best_params_"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 95,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "0.6851603068683528\n"
     ]
    }
   ],
   "source": [
    "y_pred_holdout = best_model.predict(X_holdout)\n",
    "print(roc_auc_score(y_holdout, y_pred_holdout))"
   ]
  },
